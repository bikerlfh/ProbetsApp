{
    "dev": {
        "aws_region": "us-east-1",
        "django_settings": "probetspp.probetspp.production",
        "profile_name": "eb-cli",
        "project_name": "probetspp",
        "runtime": "python3.7",
        "s3_bucket": "probetspp-zappa-dev",
        "keep_ware": false,
        "lambda_description": "Probetspp DEV lambda",
        "exclude": ["*.html", "*.csv", "*.sh"],
        "slim_handler": true,
        "apigateway_enabled": false,
        "memory_size":1536,
        "timeout_seconds": 300,
        "events": [
            {
                "function": "short_urls.test_task",
                "expression": "rate(1 minute)"
            }
        ],
        "use_precompiled_packages": false
    }
}